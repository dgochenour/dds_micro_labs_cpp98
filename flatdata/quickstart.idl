@final
@language_binding(FLAT_DATA)
struct Dimension {
    unsigned short width;
    unsigned short height;
};

@final 
@language_binding(FLAT_DATA)
struct Image {
    long long timestamp;
    Dimension dimension;
    octet img_data[30000000];
};
